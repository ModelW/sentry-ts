{
    "extends": "./playground/.nuxt/tsconfig.json",
    "compilerOptions": {
        "target": "es2018",
        "module": "esnext",
        "lib": ["dom", "es2018"]
    },
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
        "~/*": [
            "./*"
        ],
        "@/*": [
            "./*"
        ]
    }
}
